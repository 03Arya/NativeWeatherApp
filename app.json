{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": "false"
        }
      ]
    ],
    "name": "my-app",
    "slug": "my-app",
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "host.exp.exponent"
    }
  }
}
